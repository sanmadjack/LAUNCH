<Window x:Class="LAUNCH.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:l="clr-namespace:LAUNCH.WPF"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <l:Tabs x:Name="mainTabs" Grid.Row="0" Grid.Column="0">
            
        </l:Tabs>
        <GroupBox Grid.Column="0" Grid.Row="1" Header="Choose Profile">
        <l:Combo x:Name="profileCombo">
            
        </l:Combo>
        </GroupBox>
    </Grid>
</Window>
